{
  "name": "graphql-middleware-bugsnag",
  "version": "1.0.0",
  "description": "GraphQL Middleware plugin for Bugsnag",
  "files": ["dist"],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "scripts": {
    "prepublish": "npm run test",
    "build": "rimraf dist && tsc -d",
    "lint":
      "tslint --project tsconfig.json {src}/**/*.ts && prettier-check --ignore-path .gitignore {src,.}/{*.ts,*.js}",
    "test": "npm run lint && npm run build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michieldewilde/graphql-middleware-bugsnag.git"
  },
  "keywords": [
    "graphql",
    "middleware",
    "graphql-yoga",
    "bugsnag",
    "error",
    "error-reporting"
  ],
  "author": "Michiel De Wilde <mich.dewild@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michieldewilde/graphql-middleware-bugsnag/issues"
  },
  "homepage": "https://github.com/michieldewilde/graphql-middleware-bugsnag#readme",
  "devDependencies": {
    "@types/node": "10.9.4",
    "graphql": "14.0.2",
    "graphql-middleware": "1.7.3",
    "prettier": "1.14.3",
    "prettier-check": "2.0.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.16",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "bugsnag": "^2.4.3"
  }
}
